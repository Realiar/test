from rsf.tex import *

Paper('sample',color='sin sin2',hires='sin2')

Paper('rsfplot',lclass='beamer',options='onlymath',
      use='listings,lstlist',
      include=r'''
      \mode<presentation>
      {
      \beamertemplatenavigationsymbolsempty
      \setbeamersize{text margin left=0.2cm,text margin right=0.2cm}
      }
      ''',
      color=
      '''
      line sqline msincs mpsincs mbmincs dplines mbsincs sqsurf csqsurf csurf sqplsurf
      fganom ganom3 sganom ganom-7 ganom2
      ''',
#     \usetheme{progressbar}
#     \progressbaroptions{titlepage=normal,frametitle=normal,headline=none}
      hires=
      '''
      sqsurf csqsurf csurf sqplsurf sgath fganom ganom3 sganom
      ''')

Paper('tutorial',use='hyperref,listings',color='vscan-cmp vscan-signal2')

End()

