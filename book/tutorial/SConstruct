from rsf.book import *
from rsf.tex import Paper

chapters=Split(
'''
users
authors
devs
quick
''')

Book(chapters,
     author='Jeff Godwin',
     title='A brief introduction to Madagascar')

Paper('intro')

End(options='book',
   use='graphicx,color,amsmath,amssymb,amsbsy,hyperref,listings,framed,tabularx')
