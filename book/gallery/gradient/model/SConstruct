from rsf.proj import *
from rsf.gallery import gradient

gradient.get_velocity('vel')

Result('model','vel-model','Overlay')

gradient.zero_offset('zoff')

Result('zoff','grey title="Zero-Offset" ')

gradient.cmps('cmp')

Result('cmp','byte gainpanel=150 | grey3 flat=n title=CMPs frame1=1000 frame2=50 frame3=150')

gradient.shots('shot')

Result('shot','byte gainpanel=150 | grey3 flat=n title=Shots frame1=1000 frame2=50 frame3=150')

End()
