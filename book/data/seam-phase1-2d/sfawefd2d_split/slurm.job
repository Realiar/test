#!/bin/bash 

#SBATCH -o slurm.job.o%j 
#SBATCH -e slurm.job.e%j 
#SBATCH -p normal #queue to run on 
#SBATCH -N 10           #Number nodes to run on 
#SBATCH -n 151           # total number of MPI tasks.  I guess each node gets n/N
#SBATCH -t 2:00:00    #maximum time
#SBATCH -A Madagascar-workshop   #account to charge   

echo SLURM_NNODES=$SLURM_NNODES
echo SLURM_TASKS_PER_NODE=$SLURM_TASKS_PER_NODE
echo SLURM_NODELIST=$SLURM_NODELIST

unset DISPLAY
env
echo start `date`
time pscons 
echo end `date`



