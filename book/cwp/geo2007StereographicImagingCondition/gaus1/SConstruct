from rsf.proj import *
from rsf.recipes import fdmod
import itest

par=itest.param()
#par['labelattr']=' wantaxis=n '
# ------------------------------------------------------------
itest.onesou('ss',par)
itest.onesou('sx',par)
itest.flat('refl',par)
par['vprt']=-800
# ------------------------------------------------------------
itest.run(par)
# ------------------------------------------------------------
par['oangb']=0
par['nangb']=1
par['dangb']=3
itest.stereo3d(par)

End()
